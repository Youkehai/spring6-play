<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--    通过scope配置为单实例对象,默认为单实例，初始化时就会创建-->
    <bean id="scopeCustom" class="com.hs.ykh.scope.ScopeCustom" scope="singleton"></bean>
    <!--    通过scope配置为多实例对象，多实例对象，只有在获取的时候才会创建-->
    <bean id="scopeProperty" class="com.hs.ykh.scope.ScopeProperty" scope="prototype"></bean>
</beans>